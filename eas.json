{
  "cli": {
    "version": ">= 16.17.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal",
       "android": {
        "buildType": "apk"
      },
      "env": {
        "TMDB_API_KEY": "@TMDB_API_KEY",
        "TMDB_ACCESS_TOKEN": "@TMDB_ACCESS_TOKEN"
      }
    },
    "production": {
      "autoIncrement": true
    }
  },
  "submit": {
    "production": {}
  }
}
